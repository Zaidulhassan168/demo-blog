<% content_for :page_title ,"Index"%>
<section class="section">
    <div class="container">
        <% @posts.each do |post| %>
        <div class="box">
            <h1 class="title">
            <%= link_to post.title ,post %>
            </h1>
            <p>
            <%= post.content %>
            </p>
            
        </div>
        <% end %>
    </div>
</section>

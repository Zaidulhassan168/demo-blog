<% content_for :page_title ,"All Posts"%>
<section class="section">
    <div class="container">
        <% @posts.each do |post| %>
        <div class="box">
            <p><%= post.userName%> </p>
            <h1 class="title">
            <%= link_to post.title ,post %>
            </h1>
            <p>
            <%= post.content %>
            </p>
            
        </div>
        <% end %>
    </div>
    <%= will_paginate @post,renderer: BootstrapPagination::Rails %>
</section>
